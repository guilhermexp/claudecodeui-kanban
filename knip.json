{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.jsx"
  ],
  "project": [
    "src/**/*.{js,jsx,ts,tsx}",
    "server/**/*.js",
    "scripts/**/*.js"
  ],
  "ignore": [
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    
    "public/**",
    "examples/**"
  ],
  "ignoreDependencies": [],
  "ignoreExportsUsedInFile": true
}
